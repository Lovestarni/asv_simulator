<launch>
  <node pkg="asv_simulator"
        name="ASVNode"
        type="vessel.py"
        respawn="false"
        output="screen" />
  <node pkg="asv_simulator"
        name="meshnode"
        type="meshpublisher.py"
        respawn="false"
        output="screen" />
  <param name="robot_description" textfile="$(find asv_simulator)/config/models/simple.urdf" />
  <node pkg="robot_state_publisher"
        type="state_publisher"
        name="tf_broadcaster" />

  <!-- <param name="use_sim_time" type="bool" value="true" /> -->
  <node pkg="rviz"
        type="rviz"
        name="rviz"
        args="-d $(find asv_simulator)/config/rviz/config.rviz" />/>
</launch>
